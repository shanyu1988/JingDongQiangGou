{
  "manifest_version": 2,
  "name": "Chrome京东抢购",
  "version": "1.0",
  "description": "Chrome京东抢购",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "notifications",
    "activeTab",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["lib/jquery-3.2.1.min.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["lib/jquery-3.2.1.min.js","js/script.js"]
    }
  ]
}
